#include <iostream>
#include <string>
using namespace std;
int main() {
	int n,i=2;
	double k;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		k = 1.1;
		if (n == 1)
			cout << 1.1;
		else {
			while (i <= n) {
				k = k * (1 + 0.1 * i);
				i++;
			}
			cout << k;
		}
	}
}
